package com.lamichhane.switch_statement.demo;

public class Test {

	public static void main(String[] args) {
	
		byte x = 10;
		switch(x){
			case 10:
				System.out.println(10);
				break;
			case 100:
				System.out.println(100);
				break;
			case 1000:
				System.out.println(1000);
				break;
		}

	}

}
