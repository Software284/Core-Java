package com.lamichhane.values.method_in_enum;

class Test{
	public static void main(String args[]){
		Beer b = Beer.KO;

		switch(b) {
			case KF:
				System.out.println("it is children brand");
				break;
			case KO:
				System.out.println("it is light brand");
				break;
			case RC:
				System.out.println("it is not much that kick");
				break;
			case AO:
				System.out.println("buy one get one free");
				break;
		}
	}
}
