package com.lamichhane.enum_vs_switch;

class Test{
	public static void main(String args[]){
		Beer b = Beer.KF;

		switch(b) {
			case KF:
				System.out.println("it is children brand");
				break;
			case KO:
				System.out.println("it is light brand");
				break;
			case RC:
				System.out.println();
				break;
			case AO:
				System.out.println();
				break;
		}
	}
}
